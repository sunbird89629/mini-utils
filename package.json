{
  "name": "mini-utils",
  "publisher": "sunbird89629",
  "displayName": "mini-utils",
  "description": "mini-utils",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mini-utils.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "mini-utils.proxyShowCommands",
